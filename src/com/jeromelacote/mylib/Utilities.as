package com.jeromelacote.mylib{	import flash.text.TextField;	import flash.display.MovieClip;	public class Utilities extends MovieClip	{		public function Utilities()		{					}		public function writeLog(str:String) {							var screenInfoTxt:TextField = new TextField();				screenInfoTxt.x = 100;				screenInfoTxt.y = 500;				screenInfoTxt.width = 300;				screenInfoTxt.height = 100;				screenInfoTxt.border = true;				addChild(screenInfoTxt);				screenInfoTxt.text=str;			trace ("LOG: "+str);					}	}}